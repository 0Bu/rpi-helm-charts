{
  "bumpVersions": [
    {
      "filePatterns": ["**/Chart.yaml"],
      "matchStrings": ["version:\\s(?<version>[^\\s]+)"],
      "bumpType": "patch"
    }
  ],
  "packageRules": [
    {
      "matchDatasources": ["docker"],
      "matchPackageNames": ["homebridge/homebridge"],
      "versioning": "loose"
    }
  ]
}
